var x:real, y:real, z:real, z1:real, z2:real, z3:real;
begin

assume y >= -1 and y <= 1;
assume z >= 1 and z <= 2;

z1 = 1.7;
z2 = 0.2;
z3 = 1.6;

if (-x + 0.5*y - 0.25*z + 2 == 0) then
 if (x + 0.5*z - 2.5 == 0) then
  if (-0.2*x - 0.2*y + 0.1*z + 0.22 == 0) then
   z1 = x;
   z2 = y;
   z3 = z;
  endif;
 endif;
endif; 

end
