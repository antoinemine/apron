var x : real, y : real; 
begin 
assume x >= -1 and x <= 1; 
if (x>=0) then 
y = 10*x; 
else
if (x>=-1/4) then 
	y = 12.500000*x; 
	else 
	if (x>=-2/4) then 
	y = 15.000000*x + 0.625; 
	else 
	if (x>=-3/4) then 
	y = 17.500000*x + 1.875; 
	else 
	y = 20*x + 3.75;
endif;
endif;
endif;
endif;
assume y == 1;
end

