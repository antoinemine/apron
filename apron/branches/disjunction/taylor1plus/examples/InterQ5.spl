var x : real, y : real; 
begin 
assume x >= -1 and x <= 1; 
if (x>=0) then 
y = 10*x*x; 
else
if (x>=-1/5) then 
  y = -12.000000*x*x; 
  else 
  if (x>=-2/5) then 
  y = -14.000000*x*x + 0.08; 
  else 
  if (x>=-3/5) then 
  y = -16.000000*x*x + 0.32; 
  else 
  if (x>=-4/5) then 
  y = -18.000000*x*x + 0.72; 
  else 
  y = -20*x*x + 1.28;
endif;
endif;
endif;
endif;
endif;
assume y == 1;
end
