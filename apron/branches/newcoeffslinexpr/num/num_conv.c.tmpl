#ifndef MARK_XXX_ZZZ
#define MARK_XXX_ZZZ
extern inline bool boundXXX_set_boundZZZ(boundXXX_t a, boundZZZ_t b, num_internal_t intern)
{
  if (boundZZZ_infty(b)){
    boundXXX_set_infty(a,boundZZZ_sgn(b));
    return true;
  }
  else {
    return boundXXX_set_numZZZ(a,boundZZZ_numref(b),intern);
  }
