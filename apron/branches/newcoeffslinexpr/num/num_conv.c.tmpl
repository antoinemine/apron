MACRO_FOREACH XXX @AllNum
MACRO_FOREACH ZZZ @MainNum
#define MARK1_XXX
#define MARK2_ZZZ

#if defined(MARK1_XXX) && defined(MARK2_XXX)
extern inline bool boundXXX_set_boundZZZ(boundXXX_t a, boundZZZ_t b, num_internal_t intern)
{ boundXXX_set(a,b); return true; }
extern inline bool itvXXX_set_itvZZZ(itvXXX_t a, itvZZZ_t b, num_internal_t intern)
{ itvXXX_set(a,b); return true; }
