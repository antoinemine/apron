extern inline bool numIyyy_set_llfrac(numIyyy_t a, long long int i, long long int j, num_internal_t intern)
{
  long long int t;
  assert(j>0);
  t = i>=0 ? (i+j-1)/j : i/j;
  return numIyyy_set_llint(a,t,intern) && i%j==0;
}
